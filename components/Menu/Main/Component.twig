{% extends renderer.getComponentTemplate("Kwc_Menu_Expanded_Component") %}
{% block menuLink %}
    {% if m.data.componentClass != 'Kwc_Basic_LinkTag_FirstChildPage_Component' %}
        {{ renderer.componentLink(m.data) }}
    {% else %}
        <span class="{{ 'item'|bemClass }} {{ 'item'|bemClass }}--clickable">{{ m.data.name }}</span>
    {% endif %}
{% endblock %}
